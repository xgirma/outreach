{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API",
  "main": "dist",
  "scripts": {
    "backend:start": "./node_modules/.bin/webpack --colors --progress",
    "backend:debug": "DEBUG=express:* ./node_modules/.bin/webpack --colors --progress",
    "backend:test": "jest --verbose --bail --coverage --runInBand --detectOpenHandles",
    "backend:test:watch": "jest --watch --verbose",
    "backend:eslint": "./node_modules/.bin/eslint src --fix",
    "backend:prettier": "./node_modules/.bin/prettier --write \"./**/*.{js,json}\"",
    "precommit": "lint-staged husky --dev"
  },
  "keywords": ["christian", "church", "Ethiopian", "orthodox", "community", "outreach"],
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.0",
    "faker": "^4.1.0",
    "generate-password": "^1.4.0",
    "helmet": "^3.14.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.1",
    "method-override": "^3.0.0",
    "moment": "^2.22.1",
    "mongoose": "^5.3.4",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "owasp-password-strength-test": "^1.3.0",
    "swagger-ui-express": "^4.0.1",
    "validator": "^10.8.0",
    "winston": "^2.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": ["npm run backend:eslint", "npm run backend:prettier", "git add"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "dotenv-webpack": "^1.5.5",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^1.1.2",
    "jest-cli": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "1.19.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.0.0",
    "source-map-support": "^0.5.6",
    "start-server-webpack-plugin": "^2.2.5",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=6.11.0"
  }
}
