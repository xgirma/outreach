{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "prestart": "npm run -s build",
    "build": "babel src -s -D -d dist --preset es2015,stage-0",
    "start:dev": "NODE_ENV=developement nodemon -w src --exec \"babel-node src --preset es2015,stage-0\"",
    "start:prod": "NODE_ENV=production pm2 start dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --fix",
    "pretty": "./node_modules/.bin/prettier --write \"./src/**/*.js\"",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mongoose": "^5.0.17"
  },
  "lint-staged": {
    "*.{js}": [
      "npm run lint",
      "npm run pretty",
      "npm run test"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-json": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "nodemon": "^1.17.4",
    "prettier": "1.12.1",
    "swagger-ui-express": "^3.0.8"
  }
}
