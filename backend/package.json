{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "prestart": "npm run -s build",
    "build": "babel src -s -D -d dist --preset es2015,stage-0",
    "start:dev": "NODE_ENV=developement nodemon -w src --exec \"babel-node src --preset es2015,stage-0\"",
    "start:prod": "NODE_ENV=production pm2 start dist",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mongoose": "^5.0.17"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.4"
  }
}
